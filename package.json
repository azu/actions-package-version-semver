{
  "name": "actions-package-version-semver",
  "description": "GitHub action that gets all changed files in a pull request or push.",
  "version": "1.1.0",
  "private": true,
  "author": "azu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azu/actions-package-version-semver.git"
  },
  "homepage": "https://github.com/azu/actions-package-version-semver#readme",
  "bugs": {
    "url": "https://github.com/azu/actions-package-version-semver/issues"
  },
  "keywords": [
    "GitHub",
    "Actions",
    "semver",
    "npm",
    "learn"
  ],
  "main": "lib/main.js",
  "engines": {
    "node": "^12.0.0"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist/**/* lib/**/*",
    "format": "prettier --write **/*.ts",
    "format:check": "prettier --check **/*.ts",
    "package": "ncc build",
    "test": "jest",
    "all": "yarn clean && yarn build && yarn format && yarn package && yarn test"
  },
  "dependencies": {
    "@actions/core": "^1.2.2",
    "@actions/github": "^2.1.0",
    "semver": "^7.2.2"
  },
  "devDependencies": {
    "@octokit/rest": "^16.40.2",
    "@types/jest": "^25.1.1",
    "@types/node": "^12.12.29",
    "@types/semver": "^7.1.0",
    "@typescript-eslint/parser": "^2.8.0",
    "@zeit/ncc": "^0.21.0",
    "jest": "^25.1.0",
    "jest-circus": "^25.1.0",
    "js-yaml": "^3.13.1",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "ts-jest": "^25.1.0",
    "typescript": "^3.6.4"
  }
}
